perf stat -v --log-fd 23 --control=fd:24,29 -I 10 -A --scale -x ; -a -e uncore_imc_0/period=0x0,event=0x22,name=\'UNC_M_CAS_COUNT_RD[UNIT0]\'/,uncore_imc_1/period=0x0,event=0x22,name=\'UNC_M_CAS_COUNT_RD[UNIT1]\'/,uncore_imc_0/period=0x0,event=0x23,name=\'UNC_M_CAS_COUNT_WR[UNIT0]\'/,uncore_imc_1/period=0x0,event=0x23,name=\'UNC_M_CAS_COUNT_WR[UNIT1]\'/ 
